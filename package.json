    "express": "^4.18.2",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
